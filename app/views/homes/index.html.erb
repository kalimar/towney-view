<div class="col-md-offset-3">
  <% @homes.each do |home| %>
  <address>
    <div class="media">
      <div class="media-left">
        <%# unless @home.avatar %>
          <img src="http://placebear.com/100/100" alt="place ape" class="image-object">
        <%# else %>
          <!-- avatar -->
        <%# end %>
      </div>
      <div class="media-body">
        <span class="glyphicon glyphicon-user" aria-hidden="true"></span> <strong><%= home.primary_first_name %> <%= home.primary_last_name %></strong></br>
        <span class="glyphicon glyphicon-home" aria-hidden="true"></span> <%= home.street_address %></br>
        <span class="glyphicon glyphicon-phone" aria-hidden="true"></span> <%= home.primary_phone %></br>
        <span class="glyphicon glyphicon-phone-alt" aria-hidden="true"></span> <%= home.alt_phone %></br>
        <span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> <%= mail_to home.email %></br>
      </div>
    </div>
  </address>

  <% end %>
</div>